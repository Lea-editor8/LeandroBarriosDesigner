{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles/*"],
      "@iconos/*": ["src/iconos/*"],
      "@dark/*": ["src/dark/*"],
      "@funciones/*": ["src/funciones/*"],
      "@imagenes/*": ["src/imagenes/*"],
    }
  }
}
